<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="~/css/bootstrap.css" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - LitchiLearn</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/bootstrap.css" />
</head>
<body>
    <!-- Start Landing Page -->
    
<div class="landing-page">
  <div class="container">
    <div class="header-area">
      <div class="logo">LitchiLearn</div>
      <ul class="links">
        <li><a type="button" class="nav-link text-dark" asp-controller="Student" asp-action="Index" >  Home </a></li>
        <li><a type="button" class="nav-link text-dark" asp-controller="Student" asp-action="TimeTable" >  View Time Table </a></li>
        <li><a type="button" class="nav-link text-dark" asp-controller="Student" asp-action="ManageMySubject" >  Manage My Subjects</a></li>
        <li><a type="button" class="nav-link text-dark" asp-controller="Student" asp-action="ManageAccount" >  Manage Account </a></li>
        
        <li>Log Out</li>
      </ul>
    </div>
    
   
  </div>
</div>
<div class="container body content">@RenderBody()</div>
</body>
</html>