@{
    @model IEnumerable<Final_LitchiLearn.Models.TimeTable>

    ViewData["Title"] = "Time Table";
    Layout = "~/Views/Student/_LayoutTimeTable.cshtml";
}
<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Time Table</h2>
        </div>
        <div class="col-6 text-right">
            <a asp-controller="Fighter" asp-action="Create" class="btn btn-primary">Add a new fighter</a>
        </div>
    </div>
    <br />
@if (Model.Count() >0)
{
    <table>
        <thead>
            <tr>
                <th>
                    UserID
                </th>
                <th>
                    Grade
                </th>
                <th>
                    Subject ID
                </th>
                <th>
                    TimeTable
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var TimeTable in Model)
            {
                <tr>
                    <td width="35%">@TimeTable.UserID</td>
                    <td width="35%">@TimeTable.Grade</td>
                    <td width="35%">@TimeTable.SubjectID</td>
                    <td width="35%">@TimeTable.TimeTableData</td>
                   
                   
                     
                    
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p> Error Please contact a member of the Admin staff</p>
}
</div>