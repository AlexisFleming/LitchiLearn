@{
    @model IEnumerable<Final_LitchiLearn.Models.Enrol>

    
    ViewData["Title"] = "My Subjects";
    Layout = "~/Views/Student/_LayoutMySubject.cshtml";
}
@if (Model.Count() > 0)
{
    <table>
        <thead>
            <tr>
                <th>
                    SubjectID
                </th>
               
            </tr>
        </thead>
        <tbody>
            @foreach (var Enrol in Model)
            { 
                <tr>
                   
                    
                    <td width"35%">@Enrol.SubjectID</td>
                   
                    




                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p> Error Please contact a member of the Admin staff</p>
}
</div>